  <script src="${rc.contextPath}/resources/js/jquery.carouFredSel-5.2.2-packed.js"></script>
  <script src="${rc.contextPath}/resources/js/checkbox.js"></script>
  <script src="${rc.contextPath}/resources/js/radio.js"></script>
  <script src="${rc.contextPath}/resources/js/selectBox.js"></script>

  <script>
	$(document).ready(function() {
		$("select").selectBox();
	});
  </script>

 <script>
	$(document).ready(function(){
		$("#myController").jFlow({
			controller: ".control", // must be class, use . sign
			slideWrapper : "#jFlowSlider", // must be id, use # sign
			slides: "#slider",  // the div where all your sliding divs are nested in
			selectedWrapper: "jFlowSelected",  // just pure text, no sign
			width: "984px",  // this is the width for the content-slider
			height: "480px",  // this is the height for the content-slider
			duration: 400,  // time in miliseconds to transition one slide
			prev: ".slidprev", // must be class, use . sign
			next: ".slidnext", // must be class, use . sign
			auto: true	
		});
	});
  </script>
  <script>
	$(function() {
	  $('#list_product').carouFredSel({
		prev: '#prev_c1',
		next: '#next_c1',
		auto: false
	  });
          $('#list_product2').carouFredSel({
		prev: '#prev_c2',
		next: '#next_c2',
		auto: false
	  });
	  $('#list_banners').carouFredSel({
		prev: '#ban_prev',
		next: '#ban_next',
		scroll: 1,
		auto: false
	  });
	  $(window).resize();
	});
  </script>
  <script>
       $(document).ready(function(){
	      $("button").click(function(){
		     $(this).addClass('click')
	      });             
       })
  </script>
    
</head>
<body>
  <div class="container_12">
     <div id="top">

      <div class="grid_12">
        <div class="welcome">
			#if($realName == "")
				<span>欢迎来到翻书网  请</span><a href="/book-ui/user/login">登录</a>&nbsp;&nbsp;&nbsp;&nbsp;<a href="/book-ui/user/register">免费注册</a>.
			#else
				<span>您好!&nbsp;&nbsp;</span><a href="">$!realName</a>,&nbsp;&nbsp;欢迎来到翻书网!
				<a style="float:right" href="login">切换用户</a>
			#end
        </div><!-- .welcome -->
      </div><!-- .grid_6 -->
        
    </div><!-- #top -->
  
    <div class="clear"></div>
   
    <header id="branding">
      <div class="grid_3">
        <hgroup>
          <h1 id="site_logo"><a href="/book-ui/user/index" title=""><img src="${rc.contextPath}/resources/images/logo.png" alt="Online Book Store Logo"/></a></h1>
          <h2 id="site_description">Online Book Store</h2>
        </hgroup>
      </div><!-- .grid_3 -->
      
      <div class="grid_3">
        <form class="search" action="bookInfoNameGrid">
          <input type="text" name="bookName" class="entry_form" value="" placeholder="Search book here ..."/>
	</form>
	<script>
	  	$(function(){
			$(".entry_form").blur(function(){
				if($(".entry_form").val()==""){
				
				}else{
					var bookName = $(".entry_form").val();
					window.location.href="bookInfoNameGrid?bookName="+bookName;
				}
			})
		})
	  </script>
      </div><!-- .grid_3 -->
      
      <div class="grid_6">
        <ul id="cart_nav">
          <li>
            <a class="cart_li" href="">购物车</a>
            <ul class="cart_cont">
              <li class="no_border"><p>最近添加的项目</p></li>
              #if(${shoppingCarList} != "[]")
			      #foreach($card in $shoppingCarList)
	              <li>
	                <a code="$!card.bookCode"class="prev_cart js_list_book"><div class="cart_vert"><img src="$!imagesPath$!{card.bookInfoBean.bookPicture}" alt="" title="" /></div></a>
	                <div class="cont_cart">
	                  <h4>$!{card.bookInfoBean.bookName}</h4>
	                  <div class="price">$!{card.bookQuantity} x ￥$!{card.bookInfoBean.bookPrice}</div>
	                </div>
	                <a title="close" class="close js_close_car" carCode="$!{card.code}"></a>
	                <div class="clear"></div>
	              </li>
              		#end
			  #else
					<tr>
	                    <td colspan="6">
							<span style="font-size:10pt;margin-left:35%">购物车是空的</span>
						</td>
	                </tr>
				#end
	      <li class="no_border">
		<a href="shopping_cart.html" class="view_cart">查看购物车</a>
	      </li>
            </ul>
          </li>
        </ul>

        <nav class="private">
          <ul>
            <li><a href="selcetUserInfo?userName=$!realName">个人中心</a></li>
		<li class="separator">|</li>
            <li><a href="historyOrder?userCode=$!userCode">我的历史订单</a></li>
          </ul>
        </nav><!-- .private -->
      </div><!-- .grid_6 -->
    </header><!-- #branding -->
  </div><!-- .container_12 -->
  
  <div class="clear"></div>
  
  <div id="block_nav_primary">
    <div class="container_12">
      <div class="grid_12">
        <nav class="primary">
           <ul>
			<li>
              <a href="bookInfoGrid?bookType=所有分类">所有分类</a>
              <ul class="sub">
				#foreach($navAll in $navList)
					<li style="text-align:center"><a href="bookInfoGrid?bookType=$!navAll.navPareName">$!navAll.navPareName</a></li>
                #end
              </ul>
            </li>
			<li><a>></a></li>
            #foreach($nav in $navList)
				<li><a href="bookInfoGrid?bookType=$!nav.navPareName">$!nav.navPareName</a></li>
			#end
          </ul>
        </nav><!-- .primary -->
      </div><!-- .grid_12 -->
    </div><!-- .container_12 -->
  </div><!-- .block_nav_primary -->
  
  <div class="clear"></div>
  
  <section id="main" class="entire_width">
    <div class="container_12">      
       <div id="content">
		<div class="grid_12">
			<h1 class="page_title">联系我们</h1>
		</div><!-- .grid_12 -->
		
		<div class="grid_4 adress">
			<h3>地址:</h3>
			<p>1号, 长安大街, 北京, 中国</p>
			<hr>
			
			<h3>电话:</h3>
			<p>联系方式: <span>+86 18945039214</span><br/>
			<hr>
			
			<h3>Email:</h3>
			<p><span>wanglei@fanshu.com</span><br/>
                </div><!-- .adress -->
		
		<div class="grid_8">
			<form class="contact" id="feedForm">
				<h2>反馈信息</h2>
				<input type="hidden" id="feeduserName" name="userName" value="$!userName" />
				<div class="comment">
					<strong>内容:</strong><sup class="surely">*</sup><br/>
					<textarea id="feedfeedback" name="feedback"></textarea>
				</div><!-- .comment -->
				
				<div class="submit">
					<input type="submit" id="feedButton" value="提交" />
				</div><!-- .submit -->
			</form><!-- .contact -->
                </div><!-- .grid_8 -->
       </div><!-- #content -->
	   
	   <script>
	   	$(function(){
			$("#feedButton").click(function(){
				if($("#feeduserName").val() == ""){
					alert("您还未登录！");
				}else if($("#feedfeedback").val() == ""){
					alert("内容不能为空！");
				}else{
    				$.ajax
    				({
    					type:'post',
    					url:'feedbackadd',
    					data:$("#feedForm").serialize(),
    					success:function(){
    						alert("提交成功！");
    					},
    					error:function(){
    						alert("提交失败！");
    					}
    				})
				}
			})
		})
	   </script>
       
      <div class="clear"></div>
    </div><!-- .container_12 -->
  </section><!-- #main -->
  
  <div class="clear"></div>
    
