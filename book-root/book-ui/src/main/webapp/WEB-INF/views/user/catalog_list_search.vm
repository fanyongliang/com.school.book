        <div class="grid_6">
          <div class="soc">
            <a class="google"></a>
            <a class="twitter"></a>
            <a class="facebook"></a>
          </div><!-- .soc -->
        </div><!-- .grid_6 -->

        <div class="clear"></div>
      </div><!-- .container_12 -->
    </div><!-- .f_info -->
  </footer>

</body>
