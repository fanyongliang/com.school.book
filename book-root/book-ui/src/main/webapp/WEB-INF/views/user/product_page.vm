						var code = $(this).attr("code");
						var userCode = $(this).attr("userCode");
						var bookName = $(this).attr("bookName");
						var getData={
        						   bookCode:code,
        						   userCode:userCode,
        						   bookName:bookName
    					};
						$.ajax
						({
							type:'get',
							url:'bookcompareadd',
							data:getData,
							dataType:'text',
							success:function(){
								alert("添加比较列表成功!");
