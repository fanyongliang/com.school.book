								}else{
        							if($("#bookReviews").val()== ""){
    									alert("请输入评论内容！");
        							}else{
    									var bookQuality = $(this).parents().find("input[name=bookQuality]:checked").val();
    									$("#bookQuality").val(bookQuality);
										$.ajax
										({
											type:"post",
											url:"reviewsadd",
											data:$("#userReviewsForm").serialize(),
											success:function(){
												alert("提交成功!");
												history.go(0);
											},
											error:function(){
												alert("提交失败!");
											}
										});
    								}
								}
							})
						})
